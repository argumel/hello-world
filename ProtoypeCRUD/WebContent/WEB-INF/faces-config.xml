<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
 <managed-bean>
  <managed-bean-name>loginBean</managed-bean-name>
  <managed-bean-class>mx.prototype.beans.LoginBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>nuevoClienteBean</managed-bean-name>
  <managed-bean-class>mx.prototype.beans.NuevoClienteBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>buscaActualizaCliente</managed-bean-name>
  <managed-bean-class>mx.prototype.beans.BuscaActualizaCliente</managed-bean-class>
  <managed-bean-scope>view</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>carouselClient</managed-bean-name>
  <managed-bean-class>mx.prototype.beans.CarouselClient</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>fileUploadController</managed-bean-name>
  <managed-bean-class>mx.prototype.controler.FileUploadController</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>cuentasBean</managed-bean-name>
  <managed-bean-class>mx.prototype.beans.CuentasBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>/login.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>success</from-outcome>
   <to-view-id>/view/PrincipalHome.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/view/PrincipalHome.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>salir</from-outcome>
   <to-view-id>/login.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <application/>
</faces-config>
